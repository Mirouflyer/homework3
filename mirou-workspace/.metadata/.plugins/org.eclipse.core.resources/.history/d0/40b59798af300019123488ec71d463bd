package homework3;

public class StudentInformation {
	
	private String stName = null;
	private String stDateOfBirth =null;
	private String address =null;
	
	public StudentInformation (String stName) {
	 
		this.stName=stName; 
	}
	
	
	public String getStName() {
		return stName;
	}
	public void setStName(String stName) {
		this.stName = stName;
	}
	public String getStDateOfBirth() {
		return stDateOfBirth;
	}
	public void setStDateOfBirth(String stDateOfBirth) {
		this.stDateOfBirth = stDateOfBirth;
	}
	public String getAddress() {
		return address;
	}
	public void setAddress(String address) {
		this.address = address;
	}
	

}
