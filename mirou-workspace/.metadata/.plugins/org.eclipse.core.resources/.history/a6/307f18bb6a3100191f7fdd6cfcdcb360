package homework3;

public class Generated {
	
	
	private Generated() {}
	
	public static Generated getId() {
		Generated stId = new Generated();
		return stId;
		
	}
	
    StudentInformation md = new StudentInformation(); 
    
    public String generate() {
    	
    String id = md.getStName()+md.getStDateOfBirth().substring(0, 2);
    	return id;
    	
    }
}
