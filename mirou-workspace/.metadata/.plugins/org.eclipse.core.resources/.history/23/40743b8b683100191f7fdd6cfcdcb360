package homework3;

public class GenerateId {
	
	
	private GenerateId() {}
	
	public static GenerateId getId() {
		GenerateId stId = new GenerateId();
		return stId;
		
	}
	
    StudentInformation md = new StudentInformation(); 
    
    public String generate() {
    	this.md=md;
    String id = md.getStName()+md.getStDateOfBirth().substring(0, 2);
    	return id;
    	
    }
}
